#include<iostream>
using namespace std;
class abc
{
    private:
    int x,y;
    public:
    abc()
    {
        x=y=0;
        
    }
    abc(int a)
    {
        x=y=a;
    }
    abc(int a,int b)
    {
        x=a;
        y=b;
    }
    void display()
    {
        cout<<"x="<<x<<endl; 
        cout <<"y="<<y<<endl;
    }
};
int main()
{
    abc cc1;
    abc cc2(10);
    abc cc3(10,20);
    cc1.display();
    cc2.display();
    cc3.display();
    return 0;
}
